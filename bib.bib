pmid = {22701442},
URL = {http://www.frontiersin.org/Emotion_Science/10.3389/fpsyg.2012.00179/abstract},
uri = {papers://BA44C630-97D9-46E9-8167-F0FFD7E4AE14/Paper/p9628},
