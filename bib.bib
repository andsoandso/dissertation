@article{Kim:2008p2984,
author = {Soyoun Kim and Jaewon Hwang and Daeyeol Lee}, 
journal = {Neuron},
title = {prefrontal coding of temporally discounted values during intertemporal choice},
abstract = {Reward from a particular action is seldom immediate, and the influence of such delayed outcome on choice decreases with delay. It has been postulated that when faced with immediate and delayed rewards, decision makers choose the option with maximum temporally discounted value. We examined the preference of monkeys for delayed reward in an intertemporal choice task and the neural basis for real-time computation of temporally discounted values in the dorsolateral prefrontal cortex. During this task, the locations of the targets associated with small or large rewards and their corresponding delays were randomly varied. We found that prefrontal neurons often encoded the temporally discounted value of reward expected from a particular option. Furthermore, activity tended to increase when discounted values for targets were presented in the neuron's preferred direction, suggesting that activity related to temporally discounted values in the prefrontal cortex might determine the animal's behavior during intertemporal choice.},
affiliation = {Department of Neurobiology, Yale University School of Medicine, New Haven, CT 06510, USA.},
annote = {Prev work has left the early reward stationary, moving the second out until responses equalize. However when only one is stationary an the other is moving (nearly) linearly outward, the animal can easily estimate based on past experience, thus te anima is not require to calculate the out-reward val on a trial-by-trial basis (making it hard to determine where the discounting is taking place?). In this sort of linear adjustment paradigm animal are biased towards previous choices (more-or-less indep of R values). Also previous studies have seen discounting activity after choice in avain PFc and OFC (rat). Discounting is often hyperbolic, not base(e) exponential. R magintude and delay times wre varied; select reward based on eyemovement to one of two targets. (n-arm version would add?) Focus on the DLPFC (contextual control, decision making) " The results from the present study show that many neurons in the DLPFC changed their activity similarly when the magnitude of reward from a particular target decreased and when the same reward was delayed, suggesting that DLPFC encodes the temporally discounted value of reward expected from a particular choice. " "All three animals chose the small-reward target more frequently as the delay for the small reward decreased and as the delay for the large reward increased (Figures 1C and 1D), indicating that their preference for delayed reward was systematically influenced by the information about the reward delays extracted from the clocks. In addition, their choice behaviors were better accounted for by a hyperbolic discount function than by an exponential discount function. "},
