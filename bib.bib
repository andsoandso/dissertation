@article{Dahlin:2008p1559,
author = {Erika Dahlin and Anna Stigsdotter Neely and Anne Larsson and Lars B{\"a}ckman and Lars Nyberg}, 
journal = {Science},
title = {Transfer of learning after updating training mediated by the striatum},
abstract = {Process-specific training can improve performance on untrained tasks, but the magnitude of gain is variable and often there is no transfer at all. We demonstrate transfer to a 3-back test of working memory after 5 weeks of training in updating. The transfer effect was based on a joint training-related activity increase for the criterion (letter memory) and transfer tasks in a striatal region that also was recruited pretraining. No transfer was observed to a task that did not engage updating and striatal regions, and age-related striatal changes imposed constraints on transfer. These findings indicate that transfer can occur if the criterion and transfer tasks engage specific overlapping processing components and brain regions.},
affiliation = {Department of Integrative Medical Biology, Ume{\aa} University, 90187 Ume{\aa}, Sweden. erika.dahlin@physiol.umu.se},
annote = {Transfer of ability between similar but different memory tasks is variable. By stdying "updating" using the n-back and another (LM) in both healthy young and healthy old (which have been shown to have striatal changes) Dahlin et al hoped to test the hyptothesis that transfer is due to overlapping neural repressentations. Key finding: old saw no sig transfer, and no sig str activation. However the older population had less cortical activation (medial frontal, pariatal) and had about half the mean recall (across the 5 week training). Commom areas were lit up for young, n-back versus LM. What is the alternate hypothesis, no neural overlap but X? wtf? Stroop (an odd control) showed no str activation, and no transfer. However stroop is a very different task, so whether this is really any evidence at all is unclear to me (I tend to think it is not relelvant) They do conclude that more is required than "overlapping neural systems" for transfer (stroop also saw limited acitvation of frontal and pariatal. I had a better experiment in midn when we first discussed this paper, but cannot now recall it. From Alana: previous transfer research has implicated the striatum.... This is not science worthy.},
