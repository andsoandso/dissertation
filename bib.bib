@article{Cooper:2008p5238,
author = {Jeffrey C Cooper and Brian Knutson}, 
journal = {Neuroimage},
title = {Valence and salience contribute to nucleus accumbens activation},
abstract = {Different accounts of nucleus accumbens (NAcc) function have emphasized its role in representing either valence or salience during incentive anticipation. In an event-related FMRI experiment, we independently manipulated valence and salience by cuing participants to anticipate certain and uncertain monetary gains and losses. NAcc activation correlated with both valence and salience. On trials with certain outcomes, NAcc activation increased for anticipated gains and decreased for anticipated losses. On trials with uncertain outcomes, NAcc activation increased for both anticipated gains and losses but did not differ between them. These findings suggest that NAcc activation separately represents both valence and salience, consistent with its hypothesized role in appetitive motivation.},
