@article{Gerardin:2004p6548,
author = {Emmanuel Gerardin and Jean-Baptiste Pochon and Jean-Baptiste Poline and L{\'e}on Tremblay and Pierre-Fran{\c c}ois Van de Moortele and Richard Levy and Bruno Dubois and Denis Le Bihan and St{\'e}phane Leh{\'e}ricy}, 
